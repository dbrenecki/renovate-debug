{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>renovatebot/.github"],
  "enabledManagers": [
    "github-actions"
  ],
  "dependencyDashboardApproval": false,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["\\.github/workflows/checkmarx\\.yaml"],
      "matchStrings": [
        "uses: dbrenecki/renovate-debug/\\.github/actions/checkmarx@(?<currentValue>.*)"
      ],
      "datasourceTemplate": "semver"
    }
  ],
  "packageRules": [
    {
      "automerge": false,
      "matchManagers": ["github-actions"]
    },
    {
      "automerge": false,
      "matchDatasources": ["github-tags"],
      "matchPackageNames": ["dbrenecki/renovate-debug"],
      "versionCompatibility": "^(?<compatibility>.*)/(?<version>.*)$"
    }
  ]
}
